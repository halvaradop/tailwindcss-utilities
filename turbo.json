{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "test:watch": {
      "cache": false,
      "persistent": false
    },
    "clean": {
      "cache": false
    },
    "clean:dist": {
      "cache": false
    },
    "clean:modules": {
      "cache": false
    }
  },
  "ui": "tui",
  "cacheDir": ".turbo/cache"
}
